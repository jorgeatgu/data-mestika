{
  "name": "base",
  "version": "2.0.0",
  "description": "Base para cualquier proyecto de Jorge Aznar, front/designer en Zaragoza ⦉ http://jorgeatgu.com ⦊",
  "main": "gulpfile.js",
  "repository": "jorgeatgu/base",
  "keywords": [
    "css",
    "postcss",
    "gulp",
    "init"
  ],
  "author": "Jorge Aznar",
  "lint-staged": {
    "src/css/*.css": "stylelint"
  },
  "pre-commit": "lint-staged",
  "scripts": {
